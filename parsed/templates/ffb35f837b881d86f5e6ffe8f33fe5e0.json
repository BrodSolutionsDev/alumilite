          <article class=\"footer-info-col footer-info-dropdown footer-info-col--small\"\r\n                data-section-type=\"footer-webPages\">\r\n                <h5 class=\"footer-info-heading\">\r\n                    Resources\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use xlink:href=\"#icon-downselector\" />\r\n                        </svg>\r\n                    </i>\r\n                </h5>\r\n                <ul class=\"footer-info-list\">\r\n                    <li>\r\n                        <a href=\"/about/\">About</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/affiliates/\">Affiliates</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/contact-us/\">Contact Us</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/find-locally/\">Find Locally</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/shipping-returns/\">Shipping & Returns</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/support/\">Support</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/terms-conditions/\">Terms & Conditions</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/privacy-policy/\">Privacy Policy</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\r\n                    </li>\r\n                </ul>\r\n            </article>\r\n\r\n            {{#if theme_settings.social_icons_footer}}\r\n            <article class=\"footer-info-col footer-mobile-limited footer-info-col--small\"\r\n                data-section-type=\"footer-webPages\">\r\n                <h5 class=\"footer-info-heading\">{{lang 'social.connect'}}</h5>\r\n                {{> components/common/social-links}}\r\n            </article>\r\n            {{/if}}\r\n\r\n            {{#if settings.address}}\r\n            <article class=\"footer-info-col footer-mobile-limited footer-info-col--small\" data-section-type=\"storeInfo\">\r\n                <div class=\"footer-logo-image-container\">\r\n                    <img class=\"footer-logo-image\" src=\"{{cdn 'icons/alumilite-logo-light-grey.svg'}}\"\r\n                        alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n                </div>\r\n                <!--<h5 class=\"footer-info-heading footer-store-name\">{{settings.store_name}}</h5>-->\r\n                <address>2612 Kersten Court<br>Kalamazoo, MI 49048\r\n                </address>\r\n                {{#if settings.phone_number}}\r\n                <div class=\"footer-info-phone\">\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use xlink:href=\"#icon-phone\" />\r\n                        </svg>\r\n                    </i>\r\n                    <a href=\"tel:{{settings.phone_number}}\">{{settings.phone_number}}</a>\r\n                </div>\r\n                {{/if}}\r\n                <div class=\"footer-info-phone\">\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use xlink:href=\"#icon-envelope\" />\r\n                        </svg>\r\n                    </i>\r\n                    <a href=\"mailto:info@alumilite.com\">info@alumilite.com</a>\r\n                </div>\r\n                <p class=\"business-hours\">Monday-Friday<br>8:00am - 8:00pm (EST)</p>\r\n            </article>\r\n            {{/if}}\r\n        </section>\r\n        {{#if theme_settings.geotrust_ssl_common_name}}\r\n        <div class=\"footer-geotrust-ssl-seal\">\r\n            {{> components/common/geotrust-ssl-seal}}\r\n        </div>\r\n        {{/if}}\r\n\r\n        <div class=\"footer-copyright\">\r\n            {{> components/common/payment-icons}}\r\n            {{#if theme_settings.show_copyright_footer}}\r\n            <p class=\"powered-by\">1996-{{moment format=\"YYYY\"}} &copy; Alumilite Corporation. All Rights Reserved.</p>\r\n            {{/if}}\r\n        </div>\r\n\r\n    </div>\r\n</footer>\r\n","components/common/geotrust-ssl-seal":"<table width=\"135\" border=\"0\" cellpadding=\"2\" cellspacing=\"0\" title=\"Click to Verify - This site chose GeoTrust SSL for secure e-commerce and confidential communications.\">\r\n    <tr>\r\n        <td width=\"135\" align=\"center\" valign=\"top\">\r\n            <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name={{theme_settings.geotrust_ssl_common_name}}&amp;size={{theme_settings.geotrust_ssl_seal_size}}&amp;lang={{locale_name}}\"></script><br />\r\n            <a href=\"http://www.geotrust.com/ssl/\" target=\"_blank\"  style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"></a>\r\n        </td>\r\n    </tr>\r\n</table>\r\n","components/common/payment-icons":"{{#with theme_settings}}\n    {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa show_accept_amazonpay show_accept_googlepay show_accept_klarna}}\n    <div class=\"footer-payment-icons\">\n        {{#if show_accept_amex}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-american-express\"></use></svg>\n        {{/if}}\n        {{#if show_accept_discover}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-discover\"></use></svg>\n        {{/if}}\n        {{#if show_accept_mastercard}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-mastercard\"></use></svg>\n        {{/if}}\n        {{#if show_accept_paypal}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-paypal\"></use></svg>\n        {{/if}}\n        {{#if show_accept_visa}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-visa\"></use></svg>\n        {{/if}}\n        {{#if show_accept_amazonpay}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-amazonpay\"></use></svg>\n        {{/if}}\n        {{#if show_accept_googlepay}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-googlepay\"></use></svg>\n        {{/if}}\n        {{#if show_accept_applepay}}\n        <svg class=\"footer-payment-icon footer-payment-icon-apple\"><use xlink:href=\"#icon-logo-apple-pay\"></use></svg>\n        {{/if}}\n        {{#if show_accept_klarna}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-klarna\"></use></svg>\n        {{/if}}\n    </div>\n    {{/or}}\n{{/with}}\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                    <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n"}
