{"components/common/footer-custom":"{{#if banners.bottom}}\r\n{{#if settings.data_tag_enabled}}\r\n<div class=\"banners\" data-banner-location=\"bottom\">\r\n    {{#each (limit banners.bottom_metadata 1)}}\r\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\"\r\n        data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\r\n        <div data-event-type=\"promotion-click\">\r\n            {{{this.content}}}\r\n        </div>\r\n    </div>\r\n    {{/each}}\r\n</div>\r\n{{else}}\r\n<div class=\"banners\" data-banner-location=\"bottom\">\r\n    <div class=\"banner\">\r\n        {{{limit banners.bottom 1}}}\r\n    </div>\r\n</div>\r\n{{/if}}\r\n{{/if}}\r\n\r\n\r\n{{#if settings.show_newsletter_box}}\r\n<div class=\"footer-newsletter\">\r\n    <strong style=\"font-size: 18px;\">Subscribe for monthly offers and updates</strong>\r\n    <!-- <a class=\"button button--secondary\" href=\"/newsletter-signup\">Signup Here</a> -->\r\n    <div class=\"container\">\r\n        <div class=\"klaviyo-form-MtzN7Q\"></div>\r\n    </div>\r\n</div>\r\n{{/if}}\r\n\r\n<script>\r\n    const metaTags = \"{{head.meta_tags}}\";\r\n    if (metaTags.includes(\"academy\")) {\r\n        document.querySelector(\".footer-newsletter\").style.display = \"none\"\r\n    }\r\n</script>\r\n\r\n<footer class=\"footer\" role=\"contentinfo\">\r\n    <div class=\"container\">\r\n        <section class=\"footer-info 4a616d6573\">\r\n            <article class=\"footer-info-col footer-info-dropdown footer-info-col--small\"\r\n                data-section-type=\"footer-categories\">\r\n                <h5 class=\"footer-info-heading\">\r\n                    Products\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use xlink:href=\"#icon-downselector\" />\r\n                        </svg>\r\n                    </i>\r\n                </h5>\r\n   