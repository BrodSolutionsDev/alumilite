{"components/custom/blog/post":"<div class=\"flex-33\">\r\n    <div class=\"b-post\">\r\n        <div class=\"b-post-content\">\r\n            {{#if post.thumbnail}}\r\n                <div class=\"blog-thumbnail\" style=\"background-image:url({{getImage post.thumbnail 'defaultImage'}})\"><a href=\"{{post.url}}\"></a></div>\r\n            {{else}}\r\n                <div class=\"blog-thumbnail\" style=\"background-image:url(https://via.placeholder.com/600x400?text=Blog)\">\r\n                    <a href=\"{{post.url}}\"></a>\r\n                </div>\r\n            {{/if}}\r\n            <div class=\"b-post-desc\">\r\n                <h2 class=\"b-post-title\"><a href=\"{{post.url}}\">{{post.title}}</a></h2>\r\n                <a href=\"{{url}}\" class=\"button button--primary\">Read More</a>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n"}
